{
  "name": "ktodoapp-client",
  "version": "1.0.0",
  "description": "Simple todoapp on NodeJS & Svelte",
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "serve": "sirv public --single --no-clear",
    "serve:dev": "sirv public --single --no-clear --dev",
    "dev": "run-p start:dev autobuild"
  },
  "author": "Alex Krol <krolmail@list.ru>",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "18.0.0",
    "@rollup/plugin-node-resolve": "11.2.1",
    "npm-run-all": "4.1.5",
    "rollup": "2.45.2",
    "rollup-plugin-css-only": "3.1.0",
    "rollup-plugin-livereload": "2.0.0",
    "rollup-plugin-svelte": "7.1.0",
    "rollup-plugin-terser": "7.0.2"
  },
  "dependencies": {
    "@roxi/routify": "^2.15.1",
    "axios": "0.21.1",
    "sirv-cli": "1.0.11",
    "svelte": "3.37.0"
  },
  "routify": {
    "extensions": "svelte,html,md",
    "dynamicImports": true,
    "routifyDir": ".routify"
  }
}
