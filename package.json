{
  "name": "ktodoapp",
  "version": "1.0.0",
  "description": "Simple todoapp on NodeJS & Svelte",
  "scripts": {
    "install:back": "cd server && npm install",
    "install:front": "cd client && npm install",
    "install": "npm install && npm run install:back && npm run install:front",
    "dev:back": "cd server && npm run dev",
    "dev:front": "cd client && npm run dev",
    "dev": "concurrently \"npm run dev:back\" \"npm run dev:front\""
  },
  "keywords": [],
  "author": "Alex Krol <krolmail@list.ru>",
  "license": "MIT",
  "dependencies": {
    "concurrently": "6.0.2"
  }
}
