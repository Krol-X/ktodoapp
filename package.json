{
  "name": "ktodoapp",
  "version": "1.0.0",
  "description": "Simple todoapp on NodeJS & Svelte",
  "scripts": {
    "install:back": "cd server && npm install",
    "install:front": "cd client && npm install",
    "install": "npm run install:back && npm run install:front",
    "serve:back": "cd server && npm run serve",
    "serve:front": "cd client && npm run serve",
    "dev": "concurrently \"npm run serve:back\" \"npm run serve:front\""
  },
  "keywords": [],
  "author": "Alex Krol <krolmail@list.ru>",
  "license": "MIT",
  "dependencies": {
    "concurrently": "6.0.2"
  }
}
